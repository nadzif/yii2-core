{
  "name": "nadzif/yii2-core",
  "description": "Yii2 Base Component",
  "license": "MIT License",
  "authors": [
    {
      "name": "Nadzif",
      "email": "nadzif.lambda@gmail.com",
      "role": "Core Developer"
    },
    {
      "name": "Harto Tri Mulyo",
      "email": "harto.trimulyo@gmail.com",
      "role": "Core Developer"
    }
  ],
  "keywords": [
    "yii2",
    "base",
    "component",
    "action",
    "widget"
  ],
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6",
    "yiisoft/yii2": ">=2.0.47",
    "yiisoft/yii2-bootstrap4": "^2.0.4",
    "rmrevin/yii2-fontawesome": "^3.0",
    "rmrevin/yii2-ionicon": "^1.2",
    "ramsey/uuid": "^3.8",
    "kartik-v/yii2-grid": "^3.3",
    "kartik-v/yii2-widget-select2": "^2.1",
    "kartik-v/yii2-widget-datepicker": "^1.4",
    "kartik-v/yii2-date-range": "^1.7",
    "jeremeamia/superclosure": "^2.4",
    "demogorgorn/yii2-ajax-submit-button": "^1.2",
    "nesbot/carbon": "^1.39",
    "kartik-v/yii2-widget-activeform": "^1.5",
    "kartik-v/yii2-bootstrap4-dropdown": "1.0"
  },
  "autoload": {
    "psr-4": {
      "nadzif\\core\\": "src/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "kylekatarnls/update-helper": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
